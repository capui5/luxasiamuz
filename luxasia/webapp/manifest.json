{
    "_version": "1.58.0",
    "sap.app": {
        "id": "luxasia",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.11.3",
            "toolsId": "0bab64c6-499f-4256-adbc-76efce35a085"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
              "type": "sap.ui.model.resource.ResourceModel",
              "settings": {
                "bundleName": "luxasia.i18n.i18n"
              }
            },
            "storeModel": {
              "type": "sap.ui.model.json.JSONModel",
              "uri": "model/storeData.json"
            },
            "customerData": {
              "type": "sap.ui.model.json.JSONModel",
              "uri": "model/customerData.json"
            },
            "SalesData": {
              "type": "sap.ui.model.json.JSONModel",
              "uri": "model/salesData.json"
          }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "luxasia.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                  "name": "Routestoreselection",
                  "pattern": ":?query:",
                  "target": [
                    "Targetstoreselection"
                  ]
                },
                {
                  "name": "customersearch",
                  "pattern": "customersearch",
                  "target": [
                    "customersearch"
                  ]
                },
                {
                  "name": "mainmenu",
                  "pattern": "mainmenu",
                  "target": [
                    "mainmenu"
                  ]
                },
                {
                  "name": "brand",
                  "pattern": "brand",
                  "target": [
                    "brand"
                  ]
                },
                {
                  "name": "newcustomer",
                  "pattern": "newcustomer",
                  "target": [
                    "newcustomer"
                  ]
                },
                {
                  "name": "calllist",
                  "pattern": "calllist",
                  "target": [
                    "calllist"
                  ]
                },
                {
                  "name": "reviewallcampaigns",
                  "pattern": "reviewallcampaigns",
                  "target": [
                    "reviewallcampaigns"
                  ]
                },
                {
                  "name": "salescompletion",
                  "pattern": "salescompletion",
                  "target": [
                    "salescompletion"
                  ]
                }
              ],
              "targets": {
                "Targetstoreselection": {
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "storeselection",
                  "viewName": "storeselection"
                },
                "customersearch": {
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "customersearch",
                  "viewName": "customersearch"
                },
                "mainmenu": {
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "mainmenu",
                  "viewName": "mainmenu"
                },
                "brand": {
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "brand",
                  "viewName": "brand"
                },
                "newcustomer": {
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "newcustomer",
                  "viewName": "newcustomer"
                },
                "calllist":{
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "calllist",
                  "viewName": "calllist"
                },
                "reviewallcampaigns":{
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "reviewallcampaigns",
                  "viewName": "reviewallcampaigns"
                },
                "salescompletion":{
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "salescompletion",
                  "viewName": "salescompletion"
                }
        
              }
            },
            "rootView": {
              "viewName": "luxasia.view.App",
              "type": "XML",
              "async": true,
              "id": "App"
            }
          }
        }